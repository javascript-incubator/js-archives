{
  "name": "react-delayer",
  "version": "0.1.2",
  "description": "React Component with delayed prop support",
  "main": "cjs/index.js",
  "browser": "dist/asynccomponent.min.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "cjs",
    "es",
    "src"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "prepush": "npm run lint:test",
    "release": "release-it",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint src/*.js",
    "test": "cross-env BABEL_ENV=test clear && jest",
    "test:coverage": "cross-env BABEL_ENV=test jest --coverage",
    "test:watch": "clear && jest --watch",
    "lint:test": "npm run lint && npm run test:coverage",
    "purge": "rimraf es && rimraf cjs && rimraf dist",
    "build": "clear && npm run lint:test && npm run purge && cross-env BABEL_ENV=es NODE_ENV=production rollup -c"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "redux",
    "state",
    "code-splitting",
    "async-component",
    "delayed-component"
  ],
  "author": "Rajat Sharma (rajatsharma0720@gmail.com)",
  "license": "MIT",
  "repository": "rajatsharma305/react-delayer",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.12.0",
    "jest-cli": "^20.0.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "release-it": "^2.5.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.39.2",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-bundle-size": "^1.0.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  }
}
