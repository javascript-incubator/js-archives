{
  "env": {
    "test": {
      "presets": [
        "babel-preset-react"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-es2015-spread",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "cjs": {
      "plugins": ["transform-class-properties", [
          "babel-plugin-transform-runtime",
          {
            helpers: true,
            polyfill: true,
            regenerator: true
          },
        ],
        "transform-object-rest-spread",
        "add-module-exports"
      ],
      "presets": [
        ["es2015", {
          "loose": true
        }]
      ]
    },
    "es": {
      "plugins": [
        "transform-class-properties", [
          "babel-plugin-transform-runtime",
          {
            helpers: true,
            polyfill: true,
            regenerator: true
          },
        ],
        "transform-object-rest-spread"
      ],
      "presets": [
        ["es2015", {
          "loose": true,
          "modules": false
        }]
      ]
    }
  }
}
